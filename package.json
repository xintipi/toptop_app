    "dayjs": "^1.11.5",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
