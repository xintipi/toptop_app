    "prettier": "prettier --check --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "format": "npm run prettier && npm run lint:fix",
    "postinstall": "husky install",
    "pre-commit": "npm run format"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@tippyjs/react": "^4.2.6",
    "axios": "^0.27.2",
